{
        "name": "three.js extension2",
        "version": "1.0.0",
        "manifest_version": 2,
        "description": "super description",
        "devtools_page": "devtools.html",
        "background": {
                "scripts": [
                        "background.js"
                ]
        },

	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content_script.js"],
		"run_at": "document_end",
		"all_frames": true
	} ],
	"permissions": [
		"http://*/*",
		"https://*/*",
		"webNavigation"
	],
	"content_security_policy": "default-src 'self' chrome-extension-resource: ; img-src * ; script-src 'self' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; connect-src *; frame-src *;",
	"web_accessible_resources": [
		"*"
	]
}
